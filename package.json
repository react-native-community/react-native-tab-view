{
  "name": "react-native-tab-view",
  "version": "0.0.59",
  "description": "Tab view component for React Native",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "flow": "flow .",
    "prepublish": "npm run build && npm run build-flow",
    "build": "BABEL_ENV=build babel src -d lib/",
    "build-flow": "find ./src -name '*.js' -not -path '*/__tests__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow; done"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native",
    "ios",
    "android",
    "tab",
    "swipe",
    "scrollable",
    "coverflow"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satya164/react-native-tabview.git"
  },
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com> (https://github.com/satya164/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satya164/react-native-tabview/issues"
  },
  "homepage": "https://github.com/satya164/react-native-tabview#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-react-native": "^2.2.1",
    "flow-bin": "^0.36.0",
    "jest": "^18.1.0",
    "prettier": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.40.0",
    "react-test-renderer": "^15.4.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^2.0.1"
